{
  "name": "jaab",
  "version": "1.0.0",
  "description": "jaab.dev website",
  "main": "server/index.js",
  "scripts": {
    "stop": "cd server && node stop-server.js",
    "start": "cd server && node index.js",
    "test-start": "cd server && npx pm2 start index.js --name test_server",
    "test-stop": "cd server && npx pm2 delete test_server",
    "test-restart": "cd server && npx pm2 restart test_server",
