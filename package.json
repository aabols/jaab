  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "path": "^0.12.7",
    "pm2": "^5.2.2",
    "sequelize": "^6.28.0"
  }
}