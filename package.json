{
  "name": "jaab",
  "version": "1.0.0",
  "description": "jaab.dev website",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently npm:dev-*",
    "db-add-migration": "cd server/db && npx sequelize-cli migration:generate --name migration-name",
    "db-migrate": "cd server/db && npx sequelize-cli db:migrate",
    "db-add-seeder": "cd server/db && npx sequelize-cli seed:generate --name seeder-name",
    "db-seed": "cd server/db && npx sequelize-cli db:seed:all",
    "db-unseed": "cd server/db && npx sequelize-cli db:seed:undo:all",
    "stop": "cd server && node stop-server.js",
    "start": "cd server && node index.js",
    "test-start": "cd server && npx pm2 start index.js --name test_server",
    "test-stop": "cd server && npx pm2 delete test_server",
    "test-restart": "cd server && npx pm2 restart test_server",
    "dev-server": "cd server && nodemon index.js",
    "dev-client": "cd client && npm start"
  },
  "author": "Janis Abols",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "sqlite3": "^5.1.4"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.1.2",
    "path": "^0.12.7",
    "pm2": "^5.2.2",
    "sequelize": "^6.28.0",
    "sequelize-cli": "^6.6.0"
  }
}
